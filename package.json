  "homepage": "https://github.com/seiyria/gloomhavenbot#readme",
  "dependencies": {
    "discord.js": "^12.5.3",
    "dotenv": "^8.0.0",
    "fuzzyset-obj": "^1.0.0",
    "jimp": "^0.16.1",
    "ts-node": "^8.3.0",
    "typeorm": "^0.2.18",
    "typescript": "^3.5.2",
    "typescript-ioc": "^1.2.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "ava": "^2.1.0",
